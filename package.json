    "@typescript-eslint/eslint-plugin": "~5.13.0",
    "@typescript-eslint/parser": "~5.13.0",
    "eslint": "~8.10.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-self": "~1.2.1",
    "prettier": "aMarCruz/prettier#2.4.0",
    "typescript": "~4.6.2",
    "yarn": "^1.22.17"
  },
  "peerDependencies": {
    "eslint": "^8.4.1"
  },
